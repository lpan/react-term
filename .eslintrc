{
  "extends": "airbnb",
  "env": {
    "jest": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "consistent-return": 0,
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "import/imports-first": 0,
    "object-curly-spacing": ["error", "never"],
    "no-console": ["error", {allow: ["error"]}]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
